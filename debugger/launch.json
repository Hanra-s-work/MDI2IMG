    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Raw library with file and format",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/mdi2img",
            "console": "integratedTerminal",
            "args": "'${workspaceFolder}/sample_images/mdi/C_futoshi_stand.mdi' --destination='${workspaceFolder}/sample_images/out/C_futoshi_stand.png' --format='png'"
